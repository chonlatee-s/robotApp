{
  "expo": {
    "name": "นายโรบอท",
    "slug": "robotApp",
    "privacy": "public",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
